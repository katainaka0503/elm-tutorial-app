  },
  "devDependencies": {
    "elm": "^0.19.0-bugfix2",
    "elm-live": "^3.0.0"
  },
  "scripts": {
    "api": "json-server --watch db.json --port 4000",
    "build": "elm make ./src/Main.elm --output dist/app.js && cp ./src/index.html ./dist",
    "client": "cp ./src/index.html ./dist && elm-live ./src/Main.elm --output=dist/app.js --dir=dist --pushstate",
    "start": "nf start"
  },
